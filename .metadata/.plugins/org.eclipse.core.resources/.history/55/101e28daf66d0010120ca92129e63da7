spring:
  application:
    name: account_service

  datasource:
    url: jdbc:oracle:thin:@localhost:1521/XEPDB1
    username: emp123
    password: emp123
    driver-class-name: oracle.jdbc.OracleDriver

  jpa:
    hibernate:
      ddl-auto: update
      show-sql: true
      properties:
       hibernate:
         format_sql: true
         dialect: org.hibernate.dialect.Oracle12cDialect

debug: true

server:
  port: 8080

